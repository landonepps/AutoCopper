{
  "name": "autocopper",
  "private": true,
  "version": "0.3.1",
  "description": "A browser extension to cop Supreme.",
  "scripts": {
    "start": "npm run dev:chrome",
    "build": "npm run build:chrome",
    "build:chrome": "gulp pack --production --vendor=chrome",
    "build:firefox": "gulp pack --production --vendor=firefox",
    "build:opera": "gulp pack --production --vendor=opera",
    "build:edge": "gulp pack --production --vendor=edge",
    "dev": "npm run dev:chrome",
    "dev:chrome": "gulp --watch --vendor=chrome",
    "dev:firefox": "gulp --watch --vendor=firefox",
    "dev:opera": "gulp --watch --vendor=opera",
    "dev:edge": "gulp --watch --vendor=edge",
    "lint": "standard"
  },
  "standard": {
    "globals": [
      "chrome"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.x.x",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/register": "^7.0.0",
    "babel-loader": "^8.0.5",
    "chai": "4.x.x",
    "chromereload": "0.x.x",
    "debounce": "1.x.x",
    "del": "3.x.x",
    "gulp": "^3.9.1",
    "gulp-bump": "3.x.x",
    "gulp-cache": "1.x.x",
    "gulp-clean-css": "^4.x.x",
    "gulp-filter": "^5.x.x",
    "gulp-git": "^2.x.x",
    "gulp-if": "2.x.x",
    "gulp-imagemin": "^4.1.0",
    "gulp-json-transform": "0.x.x",
    "gulp-less": "^4.0.1",
    "gulp-livereload": "^4.0.1",
    "gulp-plumber": "1.x.x",
    "gulp-sass": "^4.x.x",
    "gulp-sequence": "1.x.x",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-tag-version": "1.x.x",
    "gulp-util": "3.x.x",
    "gulp-zip": "^4.x.x",
    "require-dir": "1.x.x",
    "standard": "^12.0.1",
    "vinyl-named": "1.x.x",
    "webpack": "4.x.x",
    "webpack-stream": "5.x.x",
    "yargs": "^12.x.x"
  }
}
